<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!--
    Employee Number
    -->
    <record id="sequence_employee_number_seq" model="ir.sequence">
        <field name="name">Employee number sequence</field>
        <field name="code">hr.employee.number</field>
        <field name="prefix">%(y)s</field>
        <field eval="6" name="padding"/>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="False" name="company_id"/>
    </record>
</odoo>
